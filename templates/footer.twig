<div class="footerTile">
  <h3 class="title">{{data.opts.strings.partnersTitle}}</h3>
<div id="partners" class="partners flexRow gallery">
    {% for key,partner in data.partners %}
    {% if partner ends with '.jpg' or partner ends with '.png' %}
        <img class="partner" src="{{options.baseurl}}data/partners/{{partner}}" alt="{{partner}}">
    {% endif %}
    {% endfor %}
</div></div>
<div class="footerTile devInfo">
<p>{{data.opts.strings.copyright}}</p>
</div>

<div class="footerTile">
<div id="recaptcha_container" name="recaptcha_response"></div>
</div>
